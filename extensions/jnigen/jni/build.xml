<project name="test-natives" basedir="." default="all">
	<target name="clean">
		<ant antfile="build-windows32.xml" target="clean"/>

	</target>
	
	<target name="compile-natives">
		<ant antfile="build-windows32.xml"/>

	</target>
	
	<target name="pack-natives">
		<jar destfile="../libs/test-natives.jar">
			<fileset dir="../libs/windows32" includes="test.dll"/>
		</jar>
	</target>

	<target name="all" depends="compile-natives,pack-natives"/>
</project>
